var liveUpdate;function timeParse(){var origTime=parseInt($('#nav_geupdate').attr("data-last-update"));var currentTime=Math.ceil(parseInt(new Date().getTime()/1000));var inputDiff=currentTime-origTime;if(inputDiff<60){var geString=inputDiff+" seconds ago";}else if(inputDiff>=60&&inputDiff<3600){var geString=Math.floor(inputDiff/60)+" minutes and "+(inputDiff%60)+" seconds ago";}else if(inputDiff>=3600&&inputDiff<86400){var geString=Math.floor(Math.floor(inputDiff/60)/60)+" hours, "+(Math.floor(inputDiff/60)%60)+" minutes and "+(inputDiff%60)+" seconds ago";}else{var geString=Math.floor(Math.floor(Math.floor(inputDiff/60)/60)/24)+" days, "+(Math.floor(Math.floor(inputDiff/60)/60)%24)+" hours, "+(Math.floor(inputDiff/60)%60)+" minutes and "+(inputDiff%60)+" seconds ago";}
